# Streamlit Cloud Secrets Configuration Template
# Copy this file to .streamlit/secrets.toml in Streamlit Cloud dashboard

[database]
# Example configuration - customize as needed
url = "your_database_url"
username = "your_username"
password = "your_password"

[api_keys]
# Add any API keys or tokens needed for cloud deployment
openai_key = "your_openai_api_key"
github_token = "your_github_token"

[streamlit_cloud]
# Streamlit Cloud specific settings
main_dependencies = ["streamlit", "numpy", "pandas"]
python_version = "3.9"
